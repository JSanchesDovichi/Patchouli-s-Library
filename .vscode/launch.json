    "version": "0.2.0",
    "configurations": [
        {
            "name": "pagina_web (debug mode)",
            "cwd": "pagina_web",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",

            "env": {
                "CHROME_EXECUTABLE": "/usr/bin/chromium"
            },

            "args": [
                "-d",
                "chrome",
                "--web-renderer=html"
                //"--dart-define-from-file",
                //"prod_env.json"
              ]
        },
        {
            "name": "pagina_web (profile mode)",
            "cwd": "pagina_web",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "pagina_web (release mode)",
            "cwd": "pagina_web",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}